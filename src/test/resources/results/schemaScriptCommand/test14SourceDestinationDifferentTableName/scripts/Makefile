



    
first-run-all: first-run-EMPLOYEE first-run-DEPARTMENT

run-all: run-EMPLOYEE run-DEPARTMENT

drop-all: drop-EMPLOYEE drop-DEPARTMENT

first-run-EMPLOYEE:
	$(MAKE) first-run -C EMPLOYEE

run-EMPLOYEE:
	$(MAKE) run -C EMPLOYEE

drop-EMPLOYEE:
	$(MAKE) drop -C EMPLOYEE

first-run-DEPARTMENT:
	$(MAKE) first-run -C DEPARTMENT

run-DEPARTMENT:
	$(MAKE) run -C DEPARTMENT

drop-DEPARTMENT:
	$(MAKE) drop -C DEPARTMENT

